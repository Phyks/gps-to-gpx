{
  "name": "gps-to-gpx",
  "version": "0.1.0",
  "description": "A simple tool for converting an array of GPS points to GPX.",
  "homepage": "",
  "bugs": {
    "url": "https://github.com/impatrickhooper/gps-to-gpx/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Patrick Hooper",
    "email": "patrick.d.hooper@gmail.com",
    "url": "http://patrick.hoopr.io"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/impatrickhooper/gps-to-gpx"
  },
  "scripts": {
    "start": "cross-env BABEL_ENV=commonjs babel src --watch --out-dir lib",
    "lint": "eslint src test",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "clean:commonjs": "rimraf lib",
    "clean:es": "rimraf es",
    "clean:umd": "rimraf dist",
    "clean": "npm-run-all --parallel clean:*",
    "build:commonjs": "npm run clean:commonjs && cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "npm run clean:es && cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "npm run clean:umd && cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/gps-to-gpx.js && cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/gps-to-gpx.min.js",
    "build": "npm-run-all --parallel build:*"
  },
  "dependencies": {
    "babel-runtime": "6.9.2",
    "xmlbuilder": "8.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.10.3",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-es2015-loose-native-modules": "1.0.0",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "cross-env": "1.0.8",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.9.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "mocha": "2.5.3",
    "npm-run-all": "2.2.2",
    "rimraf": "2.5.2",
    "webpack": "2.1.0-beta.13"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
